{
    "name": "Auto-Assigner",
    "author": {
        "name": "Lucus Van Blaircum",
        "email": "lvb@3sigmatechnologies.com",
        "url": "https://3sigmatechnologies.com"
    },
    "defaultLocale": "en",
    "private": false,
    "location": {
        "support": {
            "ticket_sidebar": {
                "url": "http://localhost:3000/assets/ticket-assigner.html",
                "flexible": true
            }
        }
    },
    "parameters": [
        {
            "name": "groups",
            "type": "text",
            "required": false,
            "secure": false,
            "default": ""
        },
        {
            "name": "runonall",
            "type": "checkbox",
            "required": false,
            "secure": false,
            "default": ""
        },
        {
            "name": "matchall",
            "type": "checkbox",
            "required": false,
            "secure": false,
            "default": ""
        },
        {
            "name": "exactmatch",
            "type": "checkbox",
            "required": false,
            "secure": false,
            "default": ""
        },
        {
            "name": "tagstoadd",
            "type": "text",
            "required": false,
            "secure": false,
            "default": "auto-assigned"
        }
    ],
    "version": "1.0",
    "frameworkVersion": "2.0"
}